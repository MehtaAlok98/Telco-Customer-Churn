{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python",
      "memory": 1024,
      "config": {
        "maxLambdaSize": "1024mb",
        "requirements": "api/requirements.txt"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "api/app.py" }
  ]
}
